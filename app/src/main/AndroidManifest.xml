<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.caar.dise_o">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NuevoProducto" android:parentActivityName=".Perfil" />
        <activity
            android:name=".DescripcionProducto"
            android:label="Descripcion del producto"
            android:parentActivityName=".SubCategorias" />
        <activity
            android:name=".Perfil"
            android:label="Perfil"
            android:parentActivityName=".Inicio" />
        <activity
            android:name=".Carrito"
            android:label="Carrito de Compras"
            android:parentActivityName=".Inicio" />
        <activity
            android:name=".SubCategorias"
            android:parentActivityName=".Inicio" />
        <activity android:name=".Inicio" />
        <activity
            android:name=".Register"
            android:parentActivityName=".Login" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>